[flake8]
ignore = D205, D400, I101
select = Q0, I100, I201, I202, ANN001, ANN2
exclude =
  .git
  venv
  __pycache__
  source
  outputs
  docs/*
  therapy/version.py
inline-quotes = "
import-order-style = pep8
application-import-names =
  therapy
  tests
per-file-ignores =
  tests/*:ANN001, ANN2
mypy-init-return = true
