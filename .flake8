[flake8]
ignore = D205, D400, I101
max-line-length = 88
exclude =
  .git
  venv
  __pycache__
  source
  outputs
  docs/*
  analysis/*
  codebuild/*
  therapy/version.py
inline-quotes = "
import-order-style = pep8
application-import-names =
  therapy
  tests
per-file-ignores =
  tests/*:ANN001, ANN2
mypy-init-return = true
